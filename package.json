  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "compile": "tsc --build",
    "lint": "eslint --ext ts src test",
    "copy:paste:detector": "jscpd src test typings",
    "test:unit": "TS_NODE_PROJECT=tsconfig.base.json mocha 'test/unit/**/*.test.*'",
    "test:unit:coverage": "TS_NODE_PROJECT=tsconfig.base.json nyc mocha 'test/unit/**/*.test.*'",
    "test:integration": "TS_NODE_PROJECT=tsconfig.base.json mocha 'test/integration/**/*.test.*'",
    "start": "node --enable-source-maps target/src/server.js"
  },
  "dependencies": {
    "@koa/router": "9.3.1",
    "got": "11.3.0",
    "ioredis": "4.17.3",
    "koa": "2.13.0",
    "koa-bodyparser": "4.3.0",
    "koa-pino-logger": "3.0.0",
    "lodash": "4.17.19",
    "p-pipe": "3.1.0",
    "pino": "6.3.2",
    "tslib": "2.0.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/got": "9.6.11",
    "@types/ioredis": "4.17.2",
    "@types/koa": "2.11.3",
    "@types/koa-bodyparser": "4.3.0",
    "@types/koa-compose": "3.2.5",
    "@types/koa-pino-logger": "2.1.6",
    "@types/koa__router": "8.0.2",
    "@types/lodash": "4.14.157",
    "@types/mocha": "8.0.0",
    "@types/node": "13.13.5",
    "@types/pino": "6.3.0",
    "@types/sinon": "9.0.4",
    "@types/test-listen": "1.1.0",
    "@typescript-eslint/eslint-plugin": "3.6.1",
    "@typescript-eslint/parser": "3.6.1",
    "chai": "4.2.0",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-mocha": "7.0.1",
    "eslint-plugin-prettier": "3.1.4",
    "jscpd": "3.2.1",
    "koa-compose": "4.1.0",
    "mocha": "8.0.1",
    "nyc": "15.1.0",
    "prettier": "2.0.5",
    "sinon": "9.0.2",
    "test-listen": "1.1.0",
    "ts-node": "8.10.2",
    "typescript": "3.9.6"
  }
