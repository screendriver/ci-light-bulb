{
  "name": "cibulb",
  "version": "0.1.0",
  "private": true,
  "repository": "git@github.com:screendriver/cibulb.git",
  "author": "Christian Rackerseder <git@echooff.de>",
  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "compile": "tsc --build",
    "lint": "eslint src test --ext '.ts'",
    "copy:paste:detector": "jscpd src test typings",
    "test:unit": "TS_NODE_PROJECT=tsconfig.base.json mocha 'test/unit/**/*.test.*'",
    "test:unit:coverage": "TS_NODE_PROJECT=tsconfig.base.json c8 mocha 'test/unit/**/*.test.*'",
    "test:integration": "TS_NODE_PROJECT=tsconfig.base.json mocha 'test/integration/**/*.test.*'",
    "start": "node --enable-source-maps target/src/server.js"
  },
  "dependencies": {
    "@koa/router": "9.4.0",
    "got": "11.8.0",
    "ioredis": "4.19.1",
    "koa": "2.13.0",
    "koa-bodyparser": "4.3.0",
    "koa-pino-logger": "3.0.0",
    "lodash": "4.17.20",
    "p-pipe": "3.1.0",
    "pino": "6.7.0",
    "tslib": "2.0.3"
  },
  "devDependencies": {
    "@types/chai": "4.2.14",
    "@types/got": "9.6.11",
    "@types/ioredis": "4.17.6",
    "@types/koa": "2.11.6",
    "@types/koa-bodyparser": "4.3.0",
    "@types/koa-compose": "3.2.5",
    "@types/koa-pino-logger": "3.0.0",
    "@types/koa__router": "8.0.3",
    "@types/lodash": "4.14.162",
    "@types/mocha": "8.0.3",
    "@types/node": "12.19.2",
    "@types/pino": "6.3.3",
    "@types/sinon": "9.0.8",
    "@types/test-listen": "1.1.0",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "c8": "7.3.4",
    "chai": "4.2.0",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-mocha": "8.0.0",
    "eslint-plugin-prettier": "3.1.4",
    "jscpd": "3.3.19",
    "koa-compose": "4.2.0",
    "mocha": "8.2.0",
    "prettier": "2.1.2",
    "sinon": "9.2.1",
    "test-listen": "1.1.0",
    "ts-node": "9.0.0",
    "typescript": "4.0.5"
  }
}
