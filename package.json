  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "compile": "tsc --build",
    "lint": "eslint --ext ts src test",
    "copy:paste:detector": "jscpd src test typings",
    "test:unit": "TS_NODE_PROJECT=tsconfig.base.json mocha 'test/unit/**/*.test.*'",
    "test:unit:coverage": "TS_NODE_PROJECT=tsconfig.base.json nyc mocha 'test/unit/**/*.test.*'",
    "test:integration": "TS_NODE_PROJECT=tsconfig.base.json mocha 'test/integration/**/*.test.*'",
    "start": "node --enable-source-maps target/src/server.js"
  },
  "dependencies": {
    "@koa/router": "9.4.0",
    "got": "11.5.2",
    "ioredis": "4.17.3",
    "koa": "2.13.0",
    "koa-bodyparser": "4.3.0",
    "koa-pino-logger": "3.0.0",
    "lodash": "4.17.20",
    "p-pipe": "3.1.0",
    "pino": "6.5.1",
    "tslib": "2.0.1"
  },
  "devDependencies": {
    "@types/chai": "4.2.12",
    "@types/got": "9.6.11",
    "@types/ioredis": "4.17.3",
    "@types/koa": "2.11.4",
    "@types/koa-bodyparser": "4.3.0",
    "@types/koa-compose": "3.2.5",
    "@types/koa-pino-logger": "2.1.6",
    "@types/koa__router": "8.0.2",
    "@types/lodash": "4.14.161",
    "@types/mocha": "8.0.3",
    "@types/node": "12.12.6",
    "@types/pino": "6.3.0",
    "@types/sinon": "9.0.5",
    "@types/test-listen": "1.1.0",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "chai": "4.2.0",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-mocha": "7.0.1",
    "eslint-plugin-prettier": "3.1.4",
    "jscpd": "3.3.16",
    "koa-compose": "4.1.0",
    "mocha": "8.1.3",
    "nyc": "15.1.0",
    "prettier": "2.1.1",
    "sinon": "9.0.3",
    "test-listen": "1.1.0",
    "ts-node": "9.0.0",
    "typescript": "4.0.2"
  }
