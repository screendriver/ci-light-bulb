{
  "name": "cibulb",
  "version": "0.1.0",
  "private": true,
  "repository": "git@github.com:screendriver/cibulb.git",
  "author": "Christian Rackerseder <git@echooff.de>",
  "license": "MIT",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "build": "tsc --build",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "prettier:check": "prettier --check 'src/**/*.ts?(x)' 'test/**/*.ts?(x)'",
    "copy:paste:detector": "jscpd src test typings",
    "test:ci": "yarn lint && yarn prettier:check && yarn test:unit | tap-xunit > test-results.xml && yarn test:integration",
    "test:integration": "tape -r source-map-support/register 'target/test/integration/**/*.js' | tap-nirvana",
    "test:unit:base": "tape -r source-map-support/register 'target/test/unit/**/*.js'",
    "test:unit": "yarn test:unit:base | tap-nirvana",
    "test:unit:ci": "npm run test:unit:coverage | tap-xunit > target/test-results.xml",
    "test:unit:coverage": "nyc npm run test:unit:base"
  },
  "dependencies": {
    "@sentry/node": "5.5.0",
    "got": "9.6.0",
    "loglevel": "1.6.3",
    "micro": "9.3.4",
    "mongodb": "3.2.7",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@now/node": "0.12.2",
    "@types/got": "9.6.5",
    "@types/loglevel": "1.5.4",
    "@types/micro": "7.3.3",
    "@types/mongodb": "3.1.31",
    "@types/mongodb-memory-server": "1.8.0",
    "@types/sinon": "7.0.13",
    "@types/test-listen": "1.1.0",
    "ava": "2.2.0",
    "jscpd": "2.0.15",
    "mongodb-memory-server": "5.1.9",
    "now": "15.8.7",
    "nyc": "14.1.1",
    "prettier": "1.18.2",
    "sinon": "7.3.2",
    "tap-xunit": "2.4.1",
    "test-listen": "1.1.0",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.5.3",
    "typescript-tslint-plugin": "0.5.4"
  }
}
