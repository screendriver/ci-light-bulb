{
  "all": true,
  "temp-dir": "target/.nyc_output",
  "report-dir": "target/coverage",
  "cache-dir": "target/.cache",
  "exclude-after-remap": false,
  "include": ["src/**/*.ts"],
  "exclude": [
    "src/cdk/"
  ],
  "require": ["esm", "ts-node/register/transpile-only"],
  "reporter": [
    "lcov",
    "text",
    "clover"
  ]
}
