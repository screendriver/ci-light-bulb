{
  "version": 2,
  "name": "cibulb",
  "alias": "cibulb.now.sh",
  "regions": ["bru"],
  "routes": [
    {
      "src": "/api/color",
      "methods": ["POST"],
      "dest": "/api/color/index.ts"
    },
    {
      "src": "/api/refresh",
      "methods": ["GET"],
      "dest": "/api/refresh/index.ts"
    },
    {
      "src": "/api/(.*)",
      "status": 405
    }
  ]
}
