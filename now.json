{
  "version": 2,
  "name": "cibulb",
  "alias": "cibulb.now.sh",
  "regions": ["bru"],
  "routes": [
    {
      "src": "/api/color",
      "dest": "/api/color.ts",
      "methods": ["POST"],
      "headers": {
        "Access-Control-Allow-Origin": "https://postwoman.io"
      }
    },
    {
      "src": "/api/refresh",
      "dest": "/api/refresh.ts",
      "methods": ["GET"],
      "headers": {
        "Access-Control-Allow-Origin": "https://postwoman.io"
      }
    },
    {
      "src": "/api/(.*)",
      "status": 405
    }
  ]
}
